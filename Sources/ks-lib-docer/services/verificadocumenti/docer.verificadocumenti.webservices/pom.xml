<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>it.kdm.docer</groupId>
		<artifactId>docer.services.verificadocumenti</artifactId>
		<version>Docer-2.4.6.1</version>
	</parent>
	<artifactId>docer.services.verificadocumenti.webservices</artifactId>
	<packaging>war</packaging>
	<dependencies>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>${project.parent.artifactId}.sdk</artifactId>
			<version>Docer-2.4.6.1</version>
		</dependency>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>${project.parent.artifactId}.BusinessLogic</artifactId>
			<version>Docer-2.4.6.1</version>
		</dependency>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>docer.commons.services</artifactId>
			<version>Docer-2.4.6.1</version>
		</dependency>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>docer.commons.axis2-base</artifactId>
			<version>${project.version}</version>
			<type>war</type>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>xerces</groupId>
			<artifactId>xercesImpl</artifactId>
			<version>2.8.1</version>
		</dependency>
	</dependencies>

	<build>
		<finalName>WSVerificaDocumenti</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<packagingExcludes>
						WEB-INF/lib/geronimo-activation_1.1_spec-1.0.2.jar,
						WEB-INF/lib/httpcore-4.0.jar,
						WEB-INF/lib/xalan-2.6.0.jar,
						WEB-INF/lib/log4j-1.2.13.jar,
						WEB-INF/lib/jaxen-1.1.1.jar,
						WEB-INF/lib/commons-logging-1.0.4.jar,
						WEB-INF/lib/commons-io-1.4.jar,
						WEB-INF/lib/commons-codec-1.3.jar
					</packagingExcludes>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.apache.httpcomponents</groupId>
				<artifactId>httpcore</artifactId>
				<version>4.1.2</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>