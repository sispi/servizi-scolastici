<properties>

	<!-- endpoint installazione alfresco -->
	<entry key="ALFRESCO_END_POINT_ADDRESS">http://192.168.0.25:8081/alfresco/api</entry>

	<!-- timeout in millisecondi -->
	<entry key="ALFRESCO_TIMEOUT_MILLIS">60000</entry>

	<!-- l'offset da aggiungere al docnumber assegnato da Alfresco -->
	<!-- deve essere impostato a -1 per lanciare i thread in migrazione per la fascicolazione -->
	<!-- nei WSDocer deve essere correttamente valorizzato alla fine della migrazione con l'ultimo db-id di ALFRESCO -->
	<entry key="MIGRATION_DOCNUM_OFFSET">0</entry>
	
	
	<!-- threads di fascicolazione -->
	<entry key="POOL_THREADS">25</entry>
	<!-- abilitazione orderby -->
	<entry key="ORDERBY_ENABLED">false</entry>	
	<!-- formato field per orderby PREFIX o QNAME-->
	<entry key="ORDERBY_ALFRESCO_FIELD_TYPE">PREFIX</entry>
	<!-- ordinamento della ricerca delle anagrafiche -->
	<entry key="ORDERBY_SEARCH_ANAGRAFICHE">ENTE:COD_ENTE=ASC,DES_ENTE=DESC;AOO:COD_ENTE=ASC,COD_AOO=ASC;TITOLARIO:COD_ENTE=ASC,COD_AOO=ASC,CLASSIFICA=ASC;FASCICOLO:ANNO_FASCICOLO=DESC,PROGR_FASCICOLO=ASC</entry>
		
